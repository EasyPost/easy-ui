{
  "name": "@easypost/easy-ui-icons",
  "version": "1.0.0-alpha.1",
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.js"
    },
    "./svg/*.svg": "./dist/svg/*.svg"
  },
  "files": [
    "dist/*"
  ],
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "scripts": {
    "build": "vite build --config ./vite.config.ts",
    "clean": "rm -rf .turbo dist node_modules"
  },
  "devDependencies": {
    "@material-symbols/svg-300": "^0.5.5",
    "@material-symbols/svg-400": "^0.5.5",
    "@svgr/cli": "^7.0.0",
    "@svgr/plugin-jsx": "^7.0.0",
    "glob": "^9.3.4"
  }
}
