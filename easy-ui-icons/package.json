{
  "name": "@easypost/easy-ui-icons",
  "version": "1.0.0-alpha.3",
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.js"
    },
    "./svg/*.svg": "./dist/svg/*.svg"
  },
  "files": [
    "dist/*"
  ],
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "scripts": {
    "build": "vite build --config ./vite.config.ts",
    "clean": "rm -rf .turbo dist node_modules",
    "dev": "vite build --config ./vite.config.ts --watch"
  },
  "devDependencies": {
    "@material-symbols/svg-300": "^0.7.0",
    "@material-symbols/svg-400": "^0.8.0",
    "@material-symbols/svg-600": "^0.7.0",
    "@svgr/cli": "^8.0.1",
    "@svgr/plugin-jsx": "^8.0.1",
    "glob": "^10.2.5"
  }
}
