{
  "name": "@easypost/easy-ui",
  "version": "1.0.0-alpha.14",
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.js",
      "default": "./dist/*"
    }
  },
  "sideEffects": [
    "**/*.css"
  ],
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "scripts": {
    "build": "rm -rf dist && run-p build:js build:types",
    "build:js": "vite build",
    "build:types": "tsc --build",
    "clean": "rm -rf node_modules dist .turbo",
    "dev": "npm run --prefix .. start:storybook",
    "lint": "run-p lint:*",
    "lint:js": "eslint src --ext .ts,.tsx",
    "lint:styles": "stylelint 'src/**/*.{css,scss}'",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@easypost/easy-ui-icons": "1.0.0-alpha.10",
    "@easypost/easy-ui-tokens": "1.0.0-alpha.5",
    "@react-aria/toast": "^3.0.0-beta.5",
    "@react-aria/utils": "^3.21.0",
    "@react-stately/toast": "^3.0.0-beta.1",
    "@react-types/shared": "^3.20.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.8",
    "lodash": "^4.17.21",
    "overlayscrollbars": "^2.3.0",
    "overlayscrollbars-react": "^0.5.2",
    "react-aria": "^3.29.0",
    "react-is": "^18.2.0",
    "react-stately": "^3.27.0",
    "react-syntax-highlighter": "^15.5.0",
    "use-clipboard-copy": "^0.2.0"
  },
  "peerDependencies": {
    "react": "^16.14.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.14.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/react-is": "^18.2.2",
    "@types/react-syntax-highlighter": "^15.5.7",
    "@vitejs/plugin-react": "^4.1.0",
    "glob": "^10.2.5",
    "jsdom": "^22.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.68.0",
    "vite-plugin-react-remove-attributes": "^1.0.3",
    "vite-plugin-static-copy": "^0.17.0"
  }
}
