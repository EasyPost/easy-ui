{
  "name": "@easypost/easy-ui-tokens",
  "version": "1.0.0-alpha.1",
  "exports": {
    "./js/*": {
      "types": "./dist/js/*.d.ts",
      "import": "./dist/js/*.mjs",
      "require": "./dist/js/*.js"
    },
    "./css/*": "./dist/css/*",
    "./scss/*": "./dist/scss/*",
    "./json/*": "./dist/json/*"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:tokens": "style-dictionary build",
    "build:dist": "node ../scripts/copyDistFiles.mjs",
    "clean": "rm -rf .turbo dist node_modules"
  },
  "devDependencies": {
    "style-dictionary": "^3.7.2"
  }
}
