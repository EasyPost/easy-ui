{
  "name": "@easypost/easy-ui-tokens",
  "version": "1.0.0",
  "exports": {
    "./js/*": {
      "types": "./dist/js/*.d.ts",
      "import": "./dist/js/*.mjs",
      "require": "./dist/js/*.js"
    },
    "./css/*": "./dist/css/*",
    "./scss/*": "./dist/scss/*",
    "./json/*": "./dist/json/*"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:tokens": "style-dictionary build",
    "build:pkg": "node ../scripts/copyPkgJsonForDist.mjs"
  },
  "devDependencies": {
    "style-dictionary": "^3.7.2"
  }
}
